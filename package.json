{
  "name": "@nondef/ts-oai-pmh",
  "version": "0.0.1",
  "description": "OAI-PMH harvester module for nodejs - typescript refactor",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp package*.json dist && cd dist && pnpm install --prod",
    "test": "nyc --reporter=lcov mocha --exit --require ts-node/register test/*.ts",
    "coverage:badge": "coverage-badge-creator",
    "bin": "node dist/bin/index.js",
    "document": "npx typedoc src/index.ts"
  },
  "bin": {
    "ts-boilerplate": "dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctot-nondef/oai-pmh.git"
  },
  "keywords": [
    "ts",
    "typescript",
    "oai-pmh",
    "harvesting",
    "crawl",
    "metadata"
  ],
  "author": "Christoph Hoffmann",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "@types/node": "18.11.10",
    "@types/xml2js": "^0.4.14",
    "@types/lodash": "^4.14.202",
    "chai": "^4.1.2",
    "mocha": "^10.2.0",
    "coverage-badge-creator": "^1.0.14",
    "nyc": "^15.1.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "^4.7.4",
    "typedoc": "^0.25.6"
  },
  "dependencies": {
    "commander": "9.5.0",
    "axios": "^1.6.3",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
